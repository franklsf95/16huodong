<div id="two_frame">	<div id="frame_l">		{include file="{$template}/layout/menu_box.tpl"}	</div>		<div id="frame_r">				<div class="location">			<a href="{'member'|site_url}?id={$member_base_information.member_id}">{$member_base_information.member_name}</a> > <a href="{'album/index'|site_url}?member_id={$member_base_information.member_id}">{$member_base_information.member_name}的相册</a> > <a>{$album_base_information.album_name}</a>		</div>				<div class="view_member_album_photo_box">			<ul>				{foreach name=all_album_photo_information from=$all_album_photo_information item=album_photo_information}				<li>					<div class="photo_image"><img src="{$album_photo_information.photo_image}" height="150" width="150" /></div>					<div class="photo_name"><a href="{'album/view_photo'|site_url}?id={$album_photo_information.member_photo_id}">{$album_photo_information.photo_name}</a></div>					<div class="clear"></div>				</li>				{/foreach}			</ul>			<div class="clear"></div>			<div class="pagination">				{assign var="index_page_name" value="album/view_album"}				{include file="default/layout/pagination.tpl"}			</div>			<div class="clear"></div>		</div>							</div></div>