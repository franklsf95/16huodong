<div id="two_frame">	<div id="frame_l">		{include file="{$template}/layout/menu_box.tpl"}	</div>		<div id="frame_r">				<script>			$(function() {				$( "#tabs" ).tabs();			});		</script>				<div id="tabs">			<ul>				<li><a href="#tabs-1">个人资料</a></li>			</ul>						<div id="tabs-1">				<table>					<tr>						<td>头像:</td>						<td><img src="{$member_information.image}" id="portrait_view" height="200" width="200" /></td>					</tr>										<tr>						<td>姓名:</td>						<td>{$member_information.name}</td>					</tr>										<tr>						<td>性别:</td>						<td>							{if ($member_information.gender == 'M')} 男 {/if}							{if ($member_information.gender == 'F')} 女 {/if}						</td>					</tr>										<tr>						<td>生日:</td>						<td>{$member_information.birthday}</td>					</tr>										<tr>						<td>简介:</td>						<td>{$member_information.description}</td>					</tr>										<tr>						<td>QQ:</td>						<td>{$member_information.qq}</td>					</tr>										<tr>						<td>邮箱:</td>						<td>{$member_information.email}</td>					</tr>										<tr>						<td>手机:</td>						<td>{$member_information.mobilephone}</td>					</tr>										<tr>						<td>电话:</td>						<td>{$member_information.phone}</td>					</tr>										<tr>						<td>地址:</td>						<td>{$member_information.address}</td>					</tr>										<tr>						<td>爱好:</td>						<td>{$member_information.hobby}</td>					</tr>										<tr>						<td>详细内容:</td>						<td>{$member_information.content}</td>					</tr>									</table>			</div>					</div>			</div></div>