<div id="page_frame">	<div class="left_side">		{include file="{$template}/layout/menu_box.tpl"}	</div>		<div class="right_side" id="my_page">		<div class="left">			<p><span class="title">姓名:</span><span class="fields">{$member_information.member_name}</span></p>			<p><span class="title">学校:</span><span class="fields">{$member_information.member_school_name}</span></p>			<p><span class="title">组织:</span><span class="fields">{$member_information.member_organisation}</span></p>			<p><span class="title">职务:</span><span class="fields">{$member_information.member_title}</span></p>			<p><span class="title">性别:</span><span class="fields">{if $member_information.member_gender == 'F'}女{elseif $member_information.member_gender == 'M'}男{else}未填{/if}</span></p>			<p><span class="title">生日:</span><span class="fields">{$member_information.member_birthday}</span></p>			<p><span class="title">电邮:</span><span class="fields">{$member_information.member_email}</span></p>														</div>				<div class="right">			<p><img src="{$member_information.member_image}" height="200" width="200" /></p>			<p><span class="title">QQ:</span><span class="fields">{$member_information.member_qq}</span></p>			<p><span class="title">电话:</span><span class="fields">{$member_information.member_phone}</span></p>					</div>				<div class="clear"></div>								<div class="all">			<p><span class="title">地址:</span><span class="fields">{$member_information.member_address}</span></p>			<p><span class="title">标签:</span><span class="fields">{$member_information.member_tag}</span></p>			<p><span class="title">描述:</span><span class="fields">{$member_information.member_description|default:" 暂无简介"}</span></p>		</div>												</div>	</div>