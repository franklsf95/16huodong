<script>	$(function(){		//导航提示		$('#nav_09').css('color','#ad4c4c');	});</script><div id="page_frame">	<div class="left_side">		__(include file="__($template)__/layout/menu_box.tpl")__	</div>		<div class="right_side" id="friend_page">		<ul class="friend_list">			__(foreach name=all_friend_information from=$all_friend_information item=member_information)__			__(if ($member_information.member_type == 'student' || $member_information.member_type == 'student_organization') )__			<li>				<div class="image"><a href="__('member'|site_url)__?id=__($member_information.member_id)__"><img src="__($member_information.member_image)__" width="150" height="150" /></a></div>				<p><span class="school">__($member_information.current_school_name)__</span></p>				<p><a class="name" href="__('member'|site_url)__?id=__($member_information.member_id)__">__($member_information.member_name)__</a></p>			</li>			__(elseif ($member_information.member_type == 'commonweal_organization'))__			<li>				<div class="image"><a href="__('member'|site_url)__?id=__($member_information.member_id)__"><img src="__($member_information.member_image)__" width="150" height="150" /></a></div>				<p><span>公益组织</span></p>				<p><a class="name" href="__('member'|site_url)__?id=__($member_information.member_id)__">__($member_information.member_name)__</a></p>			</li>						__(elseif ($member_information.member_type == 'company'))__			<li>				<div class="image"><a href="__('member'|site_url)__?id=__($member_information.member_id)__"><img src="__($member_information.member_image)__" width="150" height="150" /></a></div>				<p><span>公司</span></p>				<p><a class="name" href="__('member'|site_url)__?id=__($member_information.member_id)__">__($member_information.member_name)__</a></p>			</li>			__(/if)__			__(/foreach)__		</ul>			</div></div>