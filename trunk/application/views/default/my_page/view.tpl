<div id="two_frame">	<div id="frame_l">		__(include file="__($template)__/layout/menu_box.tpl")__	</div>		<div id="frame_r">				<script>			$(function() {				$( "#tabs" ).tabs();			});		</script>				<div id="tabs">			<ul>				<li><a href="#tabs-1">个人资料</a></li>			</ul>						<div id="tabs-1">				<table>					<tr>						<td>头像:</td>						<td><img src="__($member_information.image)__" id="portrait_view" height="200" width="200" /></td>					</tr>										<tr>						<td>姓名:</td>						<td>__($member_information.name)__</td>					</tr>										<tr>						<td>性别:</td>						<td>							__(if ($member_information.gender == 'M'))__ 男 __(/if)__							__(if ($member_information.gender == 'F'))__ 女 __(/if)__						</td>					</tr>										<tr>						<td>生日:</td>						<td>__($member_information.birthday)__</td>					</tr>										<tr>						<td>简介:</td>						<td>__($member_information.description)__</td>					</tr>										<tr>						<td>QQ:</td>						<td>__($member_information.qq)__</td>					</tr>										<tr>						<td>邮箱:</td>						<td>__($member_information.email)__</td>					</tr>										<tr>						<td>手机:</td>						<td>__($member_information.mobilephone)__</td>					</tr>										<tr>						<td>电话:</td>						<td>__($member_information.phone)__</td>					</tr>										<tr>						<td>地址:</td>						<td>__($member_information.address)__</td>					</tr>										<tr>						<td>爱好:</td>						<td>__($member_information.hobby)__</td>					</tr>										<tr>						<td>详细内容:</td>						<td>__($member_information.content)__</td>					</tr>									</table>			</div>					</div>			</div></div>