<div id="two_frame">	<div id="frame_l">		__(include file="__($template)__/layout/menu_box.tpl")__	</div>		<div id="frame_r">				<div class="location">			<a href="__('member'|site_url)__?id=__($member_base_information.member_id)__">__($member_base_information.member_name)__</a> > <a href="__('album/index'|site_url)__?member_id=__($member_base_information.member_id)__">__($member_base_information.member_name)__的相册</a> > <a>__($album_base_information.album_name)__</a>		</div>				<div class="view_member_album_photo_box">			<ul>				__(foreach name=all_album_photo_information from=$all_album_photo_information item=album_photo_information)__				<li>					<div class="photo_image"><img src="__($album_photo_information.photo_image)__" height="150" width="150" /></div>					<div class="photo_name"><a href="__('album/view_photo'|site_url)__?id=__($album_photo_information.member_photo_id)__">__($album_photo_information.photo_name)__</a></div>					<div class="clear"></div>				</li>				__(/foreach)__			</ul>			<div class="clear"></div>			<div class="pagination">				__(assign var="index_page_name" value="album/view_album")__				__(include file="default/layout/pagination.tpl")__			</div>			<div class="clear"></div>		</div>							</div></div>