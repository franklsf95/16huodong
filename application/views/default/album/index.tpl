<div id="two_frame">	<div id="frame_l">		__(include file="__($template)__/layout/menu_box.tpl")__	</div>		<div id="frame_r">				<script>			$(function() {				$( "#tabs" ).tabs({					selected:1,				});			});		</script>				<div id="tabs">			<ul>				<li><a href="#tabs-1">最新相册</a></li>				<li><a href="#tabs-2">我的相册</a></li>			</ul>						<div id="tabs-1" class="index_tabs">								<div class="view_new_album_box">					<ul>						__(foreach name=all_new_album_information from=$all_new_album_information item=new_album_information)__						<li>							<div class="album_image"><img src="__($new_album_information.album_image)__" height="150" width="150" /></div>							<div class="album_name"><a href="__('album/view_album'|site_url)__?id=__($new_album_information.member_album_id)__&member_id=__($new_album_information.member_id)__">__($new_album_information.album_name)__</a></div>						</li>						__(/foreach)__					</ul>					<div class="clear"></div>				</div>							</div>						<div id="tabs-2" class="index_tabs">							<div class="view_member_album_box">					<ul>						__(foreach name=all_member_album_information from=$all_member_album_information item=member_album_information)__						<li>							<div class="album_image"><img src="__($member_album_information.album_image)__" height="150" width="150" /></div>							<div class="album_name"><a href="__('album/view_album'|site_url)__?id=__($member_album_information.member_album_id)__&member_id=__($member_album_information.member_id)__">__($member_album_information.album_name)__</a></div>						</li>						__(/foreach)__					</ul>					<div class="clear"></div>				</div>						</div>					</div>											</div></div>