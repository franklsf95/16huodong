<div id="two_frame">	<div id="frame_l">		{include file="{$template}/layout/menu_box.tpl"}	</div>		<div id="frame_r">				<script>			$(function() {				$( "#tabs" ).tabs({					selected:1,				});			});		</script>				<div id="tabs">			<ul>				<li><a href="#tabs-1">最新相册</a></li>				<li><a href="#tabs-2">我的相册</a></li>			</ul>						<div id="tabs-1" class="index_tabs">								<div class="view_new_album_box">					<ul>						{foreach name=all_new_album_information from=$all_new_album_information item=new_album_information}						<li>							<div class="album_image"><img src="{$new_album_information.album_image}" height="150" width="150" /></div>							<div class="album_name"><a href="{'album/view_album'|site_url}?id={$new_album_information.member_album_id}&member_id={$new_album_information.member_id}">{$new_album_information.album_name}</a></div>						</li>						{/foreach}					</ul>					<div class="clear"></div>				</div>							</div>						<div id="tabs-2" class="index_tabs">							<div class="view_member_album_box">					<ul>						{foreach name=all_member_album_information from=$all_member_album_information item=member_album_information}						<li>							<div class="album_image"><img src="{$member_album_information.album_image}" height="150" width="150" /></div>							<div class="album_name"><a href="{'album/view_album'|site_url}?id={$member_album_information.member_album_id}&member_id={$member_album_information.member_id}">{$member_album_information.album_name}</a></div>						</li>						{/foreach}					</ul>					<div class="clear"></div>				</div>						</div>					</div>											</div></div>