<?php
include_once "base_action_controller.php";
Class Import_school Extends CI_Controller {

	var $applicationFolder = "import_school"; 
	
	function __construct() {
		parent::__construct();
		
	}
	
	function index(){
		
		exit();
		$file=file_get_contents("school.txt","r");
		
		$file_array = explode("\r",$file);
		foreach ($file_array as &$vo) {
			$vo = trim($vo);
		
			$data = array();
			$data['province'] = "1";
			$data['area'] = "19";
			$data['type'] = "middle_school";
			$data['name'] = $vo;
			$this->db->insert('public_school',$data);
		
		}
		
	}
	
	
	
	
}



?>